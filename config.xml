<?xml version='1.0' encoding='utf-8'?>
<widget id="it.y3web.BSyncPush" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Storci Sync</name>
    <description>
        Storci Sync Application
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="icon.png" />
    <icon height="57" platform="ios" src="res/icon/ios/icon-57.png" width="57" />
    <icon height="72" platform="ios" src="res/icon/ios/icon-72.png" width="72" />
    <icon height="114" platform="ios" src="res/icon/ios/icon-57-2x.png" width="114" />
    <icon height="144" platform="ios" src="res/icon/ios/icon-72-2x.png" width="144" />
    <icon platform="blackberry" src="res/icon/blackberry/icon-80.png" />
    <icon gap:state="hover" platform="blackberry" src="res/icon/blackberry/icon-80.png" />
    <icon platform="webos" src="res/icon/webos/icon-64.png" />
    <icon platform="winphone" src="res/icon/windows-phone/icon-48.png" />
    <icon platform="winphone" role="background" src="res/icon/windows-phone/icon-173-tile.png" />
    <splash height="480" platform="ios" src="res/screen/ios/screen-iphone-portrait.png" width="320" />
    <splash height="960" platform="ios" src="res/screen/ios/screen-iphone-portrait-2x.png" width="640" />
    <splash height="1136" platform="ios" src="res/screen/ios/screen-iphone-portrait-568h-2x.png" width="640" />
    <splash height="1004" platform="ios" src="res/screen/ios/screen-ipad-portrait.png" width="768" />
    <splash height="748" platform="ios" src="res/screen/ios/screen-ipad-landscape.png" width="1024" />
    <splash src="res/screen/ios/Default@2x~universal~anyany.png" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="iosPersistentFileLocation" value="Library" />
    <engine name="ios" spec="^4.5.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-device" spec="^1.1.7" />
    <plugin name="cordova-plugin-dialogs" spec="^1.3.4" />
    <plugin name="cordova-plugin-file" spec="^5.0.0" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.7.0" />
    <plugin name="cordova-plugin-inappbrowser" spec="^1.7.2" />
    <plugin name="cordova-plugin-insomnia" spec="^4.3.0" />
    <plugin name="cordova-plugin-media" spec="^4.0.0" />
    <plugin name="cordova-plugin-network-information" spec="^1.3.4" />
    <plugin name="cordova-plugin-splashscreen" spec="^4.1.0" />
    <plugin name="plugin-document-interaction" spec="file:///Users/giovanni/Documents/work/plugin-document-interaction" />
</widget>
